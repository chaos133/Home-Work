#include <stdio.h>
#define N 5
#define M 6

void main()
{
    int A[N][M];
    for (int i = 0;i<5;i++)
    {
        for(int j = 0;j<6;j++)
        {
            if(j == 0 || i == 0||i==4||j==5){
                A[i][j] = 1;
            }
            else A[i][j] = rand()%2;
            printf("%d ",A[i][j]);
        }
        printf("\n");
    }
}
